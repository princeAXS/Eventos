    'logo_url': 'https://img.evbuc.com/https%3A%2F%2Fimg.evbuc.com%2Fhttp%253A%252F%252Fcdn.evbuc.com%252Fimages%252F14818961%252F77500563817%252F1%252Foriginal.jpg%3Frect%3D0%252C0%252C1050%252C525%26s%3D4647b805cc7428da92c15022344db71f?h=200&w=450&s=9fd2b6e37f09e7d827a5814ae057ac85',
